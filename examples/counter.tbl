task count(initial: u32, max: u32) {
    state: {
        count: u32,
        max: u32
    }
    enter: {
        state.count = initial;
        state.max = max;
    }
    repeat: {
        if state.count < state.max {
            state.count = state.count + 1;
        } else {
            exit;
        }
    }
    exit: {

    }
}

schedule main {
    count(0, 10);
}