@using("examples/std.tbl");

task foo(n: &u32) {
    printf("Hello, foo %d\n", *n);
    *n = *n + 1;
    return;
}

task bar(n: &u32) {
    printf("Hello, bar %d\n", *n);
    *n = *n + 1;
    return;
}

task main() -> i32 {
    var n: u32 = 0;
    schedule foo(&n);
    schedule bar(&n);
    //sched_run();
    return 0;
}